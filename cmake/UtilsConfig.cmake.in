@PACKAGE_INIT@

# Check whether the requested COMPONENTS are available
set(Utils_KNOWN_COMPONENTS ConfigReader Logger)
foreach(comp ${Utils_FIND_COMPONENTS})
    if(NOT comp IN_LIST Utils_KNOWN_COMPONENTS)
        set(Utils_FOUND False)
        set(Utils_NOT_FOUND_MESSAGE "Unknown component: ${comp}")
    endif()
endforeach()

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/UtilsTargets.cmake")

# Set up the Utils::Utils target if not already available
if(NOT TARGET Utils::Utils)
    set(Utils_FOUND False)
    set(Utils_NOT_FOUND_MESSAGE "Utils::Utils target not available")
endif()

check_required_components(Utils)
